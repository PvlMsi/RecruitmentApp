<h1>Logowanie</h1>

<%= form_for(:session, url: login_path) do |form| %>
    <p>
      <%= form.label "E-Mail:" %>
      <%= form.email_field :email %>
    </p>
    <p>
      <%= form.label "Hasło:" %>
      <%= form.password_field :password, autocomplete: "off" %>
    </p>
    <p>
      <%= form.submit "Zaloguj", class: "btn btn-default-sm" %>
      <%= link_to "Powrót", home_path, class: "btn btn-default-sm" %>
    </p>
<% end %>