<div class="jumbotron">
  <div class="container">
    <div class="col-xs-4 col-xs-offset-4">
      <h2 align="center">Aktualne działy</h2>
      <ul class="list-group">
        <% @division.each do |division| %>
            <li class="list-group-item"><%= division.divisionName %><%= link_to "x", division_path(division), method:  :delete, data: {confirm: "Czy jesteś pewny/a?"}, class:"close" %></li>
        <% end %>
      </ul>

      <%= link_to 'Dodaj dział', newDivision_path, type:"button", class:"btn btn-primary btn-lg btn-block" %>
      <%= link_to 'Powrót', settings_path, type:"button", class:"btn btn-default btn-lg btn-block" %>
    </div>
  </div>
</div>